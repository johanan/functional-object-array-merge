  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.10",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.0",
    "@types/ramda": "^0.27.64",
    "babel-jest": "^27.4.6",
    "jest": "^27.4.7",
    "ramda21": "npm:ramda@0.21.0",
    "typescript": "^4.5.4"
